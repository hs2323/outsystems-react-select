{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist/*", "node_modules/*", "output/*"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["node", "vite/client", "vitest/globals"],
    "paths": {
      "~/*": ["./src/*"]
    }
  }
}
